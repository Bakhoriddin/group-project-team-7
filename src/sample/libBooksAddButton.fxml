<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <AnchorPane prefHeight="343.0" prefWidth="606.0">
         <children>
            <TableView prefHeight="351.0" prefWidth="600.0">
              <columns>
                <TableColumn prefWidth="98.0" text="ISBN" />
                <TableColumn prefWidth="97.0" text="Title" />
                  <TableColumn minWidth="0.0" prefWidth="88.0" text="Author" />
                  <TableColumn prefWidth="86.0" text="Subject" />
                  <TableColumn prefWidth="109.0" text="Publish Date" />
                  <TableColumn prefWidth="121.0" text="Status" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
         <children>
            <Button fx:id="btnCancel" layoutX="490.0" layoutY="12.0" mnemonicParsing="false" onAction="#onCancelLBook" prefHeight="25.0" prefWidth="70.0" text="Cancel" />
            <Button layoutX="390.0" layoutY="12.0" mnemonicParsing="false" onAction="#onAddLBook" prefHeight="25.0" prefWidth="70.0" text="Add" />
         </children>
      </AnchorPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
